    <define name="reformattingQuality">
        <choice>
            <notAllowed/>
            <element name="reformattingQuality">
                <choice>
                    <value type="string">preservation</value>
                    <value type="string">access</value>
                    <value type="string">replacement</value>
                </choice>
            </element>
        </choice>
    </define>
    <define name="edition">
        <choice>
            <notAllowed/>
            <element name="edition">
                <ref name="anonymous1"/>
            </element>
        </choice>
    </define>
    <define name="projection">
        <choice>
